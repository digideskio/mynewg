<table class="table table-striped table-responsive table-bordered table-hover table-form">
  <thead>
    <tr>
      <th>Price Id</th>
      <th>Role</th>
      <th>Value</th>
      <th></th>
    </tr>
  </thead>
  <tbody id="package-price-commission-fields">
    <% @package.commissions.active.each do |commission| %>
      <tr id="commission_<%= commission.id %>" class="price_<%= commission.package_price.id %>">
        <%= render partial: 'admin/packages/commissions/single', format: [:html], locals: { commission: commission } %>
      </tr>
    <% end %>
  </tbody>
</table>
